/***************************************
 * ESLINT'S CONFIGURATION FOR CHECKING THE JS SYNTAX DURING WEBPACK COMPILATION
 **************************************/
{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off",
    "quotes": ["warn", "single"]
  }
}